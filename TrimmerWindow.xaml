<Window x:Class="DNA.NETCORE3._0.TrimmerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DNA.NETCORE3._0"
        mc:Ignorable="d"
        Title="Window2" Height="1080" Width="1920">
    <Grid>
        <TextBlock x:Name="PercentageSetting" Text="{Binding PercentageData}" HorizontalAlignment="Left" Margin="185,159,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FFFD0000"/>
        <Button x:Name="BackToMainWindow" Content="Back" HorizontalAlignment="Left" VerticalAlignment="Top" Width="45" Click="BackToMainWindow_Click"/>

    </Grid>
</Window>
